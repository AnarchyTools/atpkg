;; An overlay sample file

(package
  :name "overlays_src"
  :version "0.1.0-dev"
  :import ["overlays_dst.atpkg"]

  :overlays {
    :sample-overlay {
      :compile-options ["-D" "MORE_AWESOME"]
    }
  }

  :tasks {
    :build {
      :tool "lldb-build"
      :name "json-swift"
      :output-type "lib" 
      :source ["src/**.swift" "lib/**.swift"]
      :compile-options ["-D" "AWESOME"]
      :use-overlays ["sample-overlay" "overlays_dst.exported-overlay"]
    }
  }
)

; End of the sample.