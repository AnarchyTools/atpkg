;; A mixin sample file

(package
:name "overlay"
:version "0.1.0-dev"

:overlays {
    :sample-overlay {
        :compileOptions ["-D" "MORE_AWESOME"]
    }
    :sample-overlay-two {
        :compileOptions ["-D" "MOST_AWESOME"]
    }
}

:tasks {
    :build {
        :tool "lldb-build"
        :name "json-swift"
        :output-type "lib" 
        :source [ "src/**.swift" "lib/**.swift" ]
        :compileOptions ["-D" "AWESOME"]
        :overlays ["sample-overlay"]
        :configurations {
            :awesome ["sample-overlay-two"]
        }
    }
}
)

; End of the sample.