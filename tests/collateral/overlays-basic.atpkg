;; A mixin sample file

(package
  :name "overlay"
  :version "0.1.0-dev"

  :overlays {
    :data-types {
      :array ["happy" "days"]
      :map {
        :key "value"
        :nested {
          :ok true
        }
      }
      :string "string-value"
      :integer 1234
      :float 3.455
      :bool false
    }
    :append {
      :compile-options ["-D" "APPEND_FLAG"]
    }
  }

  :tasks {
    :build {
      :tool "lldb-build"
      :name "json-swift"
      :output-type "lib" 
      :source ["src/**.swift" "lib/**.swift"]
      :compile-options ["-j8"]
    }
  }
)

; End of the sample.