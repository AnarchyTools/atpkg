;; A mixin sample file

(package
:name "mixins_src"
:version "0.1.0-dev"
:import ["mixins_dst.atpkg"]

:mixins {
    :sample-mixin {
        :compileOptions ["-D" "MORE_AWESOME"]
    }
}

:tasks {
    :build {
        :tool "lldb-build"
        :name "json-swift"
        :output-type "lib" 
        :source [ "src/**.swift" "lib/**.swift" ]
        :compileOptions ["-D" "AWESOME"]
        :mixins ["sample-mixin" "mixins_dst.exported-mixin"]
    }
}
)

; End of the sample.